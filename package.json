    "clean": "rm -rf ./build && mkdir build",
    "watch": "node index.js",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyazo/sump-thing-backend.git"
